                   @Html.Partial("_CMSPartial")
                    <div class="col-md-3 col-sm-3">
                        <div class="footer_box">
                            <h1>services</h1>
                            <ul>
                                <li><a href="#">Fault Diagnostics</a></li>
                                <li><a href="#">Virus Protection </a></li>
                                <li><a href="#">Virus/Spyware/Malware Removal</a></li>
                                <li><a href="#">Performance Tune-up</a></li>
                                <li><a href="#">Software Installation</a></li>
                                <li><a href="#">Software Troubleshooting</a></li>
                                <li><a href="#">Printer Installation & Troubleshooting</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-3">
                        <div class="footer_box">
                            <h1>contact us</h1>
                            <p><strong>Address: </strong>Lorem ipsum</p>
                            <p><strong>Phone: </strong><span>(123) 456 7890</span></p>
                            <p><strong>E-mail: </strong><a href="mailto:oiiointernational.com">oiiointernational.com</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End: footer_top -->
        <div class="footer_bottom">
            <!-- Start: footer_bottom -->
            <div class="container">
                <p>All contents copyright © OiiO Haat 2015. All rights reserved Website Design,Development and SEO by -Oiio International.com</p>
                <a href="#">
                    <img src="/images/payment.png" class="img-responsive" alt="" />
                </a>
            </div>
        </div>
        <!-- End: footer_bottom -->
    </div>
    <!-- End: footer -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">
        $(document).ready(function () {
            $('#menu').slicknav();
        });
    </script>
    <script>
        $(document).ready(function () {
            $('.bxslider').bxSlider({
                mode: 'fade',
                auto: true,
                pager: false,
                controls: false
            });
            $('.lastesad_slider').bxSlider({
                maxSlides: 4,
                moveSlides: 1,
                slideWidth: 262,
                slideMargin: 30,
                controls: false,
                responsive: true,
                pager: false,
                auto: true,
            });
            $('#main_slider').flexslider({
                animation: "slide",
                directionNav: false,
                controlNav: false,
                auto: false
            });
            $('.prev, .next').on('click', function () {
                var href = $(this).attr('href');
                $('#main_slider').flexslider(href)
                return false;
            });
        });
    </script>

    @RenderSection("scripts", required: false)

</body>
</html>

