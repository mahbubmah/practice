                        <a class="select_loc" href="#"><i class="fa fa-map-marker"></i>Select Locations</a>
                    </div>
                    <div class="col-md-3 col-sm-3">
                        <a class="select_loc" href="#"><i class="fa fa-tag"></i>Home & Garden</a>
                    </div>
                    <div class="col-md-6 col-sm-6">
                        <div class="searchtop_right">
                            <input type="text" class="searchtop_Rinput" placeholder="What are you searching for?">
                            <input class="searchtop_Rbtn" type="button" value="Search">
                        </div>
                    </div>
                </div>
                <!-- End: search_top -->
                <div class="detail_content">
                    <!-- Start: detail_content -->
                    <div class="col-md-3 col-sm-3">
                        <div class="detail_left">
                            <!-- Start: detail_left -->
                            <a class="post_adbtn" href="#">Post Your Free  Ad</a>
                            <div class="detail_category">
                                <h1>All categories</h1>
                                <ul>
                                    <li><a href="#">Electronics <span>113912</span></a></li>
                                    <li><a href="#">Home Appliances <span>42352</span></a></li>
                                    <li><a href="#">Property <span>21749</span></a></li>
