                                    <li><a href="#">Leisure, Sport & Hobby <span>5964</span></a></li>
                                    <li><a href="#">Education <span>4713</span></a></li>
                                    <li><a href="#">Other <span>1158</span></a></li>
                                    <li><a href="#">Food & Agriculture<span> 422</span></a></li>
                                </ul>
                            </div>
                            <div class="detail_category">
                                <h1>‹ All The world</h1>
                                <ul>
                                    <li>
                                        <a href="#">Banladesh</a>
                                        <ul>
                                            <li><a href="#">Dhaka <span>113912</span></a></li>
                                            <li><a href="#">Chittagong <span>42352</span></a></li>
                                            <li><a href="#">Rajshahi <span>21749</span></a></li>
                                            <li><a href="#">Khulna <span>15520</span></a></li>
                                            <li><a href="#">Barishal <span>15412</span></a></li>
                                            <li><a href="#">Sylhet <span>8980</span></a></li>
                                            <li><a href="#">Rangpur <span>8816</span></a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <!-- End: detail_left -->
                    </div>
                    <div class="col-md-6 col-sm-6">
                       @RenderBody()
                    </div>

                    <div class="col-md-3 col-sm-3 right_ad">
                        <a href="#">
                            <img class="img-responsive" src="/images/ad_00.jpg" alt="" />
                        </a>
                        <a href="#">
                            <img class="img-responsive" src="/images/ad_01.jpg" alt="" />
                        </a>
                        <a href="#">
                            <img class="img-responsive" src="/images/ad_02.jpg" alt="" />
                        </a>
                    </div>
                </div>
                <!-- End: detail_content -->
            </div>
            <!-- End: content_detail -->
        </div>
        <!-- End: container -->
    </div>
    <!-- End: content_body -->

    <div>
    </div>
    <div class="footer">
        <!-- Start: footer -->
        <div class="footer_top">
            <!-- Start: footer_top -->
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3">
                        <div class="footer_box">
                            <h1>Newsletter</h1>
