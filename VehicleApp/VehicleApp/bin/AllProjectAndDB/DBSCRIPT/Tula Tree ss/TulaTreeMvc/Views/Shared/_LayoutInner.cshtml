        <!-- End: header_top -->
        <div class="header_detailmid">
            <!-- Start: header_detailmid -->
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3">
                        <div class="logo_detail">
                            <h1>
                                <a href="~/">
                                    <img src="/images/logo.png" class="img-responsive" alt="" />
                                </a>
                            </h1>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-9">
                        <div class="search_detail">
                            <div class="row">
                                <div class="col-md-3 col-sm-3">
                                    <div class="select-style">
                                        <select>
                                            <option value="active">Ad</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-3">
                                    <div class="select-style">
                                        <select>
                                            <option value="active">Bangladesh</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-3">
                                    <div class="select-style">
                                        <select>
                                            <option value="active"><< All locations in world >></option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-3">
                                    <input type="text" class="search_input" placeholder="Search.....">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End: header_detailmid -->
    </div>
    <!-- End: Header -->

    <div class="content_body">
        <!-- Start: content_body -->
        <div class="container">
            <!-- Start: container -->
            <div class="content_detail">
                <!-- Start: content_detail -->
                <div class="search_top">
